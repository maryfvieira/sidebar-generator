{
  "compilerOptions": {
    "target": "es6",
    "resolveJsonModule": true,
    "lib": [
      "es6",
      "dom"
    ],
    "types": [
      "reflect-metadata",
      "swagger-express-ts"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": "src",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "emitDecoratorMetadata": true
  }
}